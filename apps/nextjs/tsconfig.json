{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "src/types/**/*.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
